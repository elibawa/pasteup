{
    "packages":{
        "pasteup":{
            "type":"aws-s3",
            "apps":[ "aws-s3" ],
            "data":{
                "bucketResource": "pasteup",
                "cacheControl": [
                { 
                    "pattern": "^/js/lib/",
                    "value": "max-age=315360000"
                },
                {
                    "pattern": "^/\d*/",
                    "value": "max-age=315360000"
                },
                {
                    "pattern": ".*",
                    "value": "max-age=3600"
                }]
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["staticFilesUpload"]
        },
        "staticFilesUpload":{
            "actionsBeforeApp": ["pasteup.uploadStaticFiles"]
        }
    }
}