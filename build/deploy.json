{
    "packages":{
        "pasteup":{
            "type":"aws-s3",
            "apps":[ "aws-s3" ],
            "data":{
                "bucket":"pasteup-qa"
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["staticFilesUpload"]
        },
        "staticFilesUpload":{
            "actionsBeforeApp": ["pasteup.uploadStaticFiles"]
        }
    }
}