{
    "packages":{
        "pasteup":{
            "type":"aws-s3",
            "data":{
                "prefixStage": false,
                "prefixPackage": false,
                "bucketResource": "aws-bucket",
                "cacheControl": [
                { 
                    "pattern": "^js/lib/",
                    "value": "max-age=315360000"
                },
                {
                    "pattern": "^(\d+\\.)?(\d+\\.)?(\\*|\d+)/",
                    "value": "max-age=315360000"
                },
                {
                    "pattern": ".*",
                    "value": "max-age=3600"
                }]
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["staticFilesUpload"]
        },
        "staticFilesUpload":{
            "actionsBeforeApp": ["pasteup.uploadStaticFiles"]
        }
    }
}